              // begin::Item
              li.breadcrumb-item
                span.bullet.bg-gray-500.w-5px.h-2px
              // end::Item
              // begin::Item
              li.breadcrumb-item.text-muted Projects
              // end::Item
            // end::Breadcrumb
          // end::Page title
          // begin::Actions
